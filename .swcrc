{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "target": "es2022",
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "experimental": {
      "cacheRoot": ".cache/swc"
    }
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": "inline",
  "minify": false,
  "isModule": true
}
